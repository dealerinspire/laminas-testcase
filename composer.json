{
    "name": "dealerinspire/laminas-testcase",
    "description": "Unit test support module for Laminas projects",
    "require": {
        "php": "~7.0",
        "fzaninotto/faker": "^1.8",
        "zendframework/zend-test": "^2.6",
        "zendframework/zend-servicemanager": "^2.7",
        "zendframework/zend-stdlib": "*",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "<7.0.0",
        "guzzlehttp/guzzle": "6.*"
    },
    "require-dev": {
        "api-skeletons/zf-oauth2-doctrine": "*",
        "doctrine/orm": "*",
        "zendframework/zend-developer-tools": "^2.0",
        "zendframework/zend-serializer": "^2.9",
        "zendframework/zend-log": "^2.12"
    },
    "support": {
        "email": "sysops@dealerinspire.com",
        "source": "https://www.github.com/dealerinspire/laminas-testcase.git"
    },
    "config": {
        "process-timeout": 5000
    },
    "autoload": {
        "psr-4": {
            "DealerInspire\\LaminasTestcase\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DealerInspire\\LaminasTestcase\\Test\\": "test"
        }
    }
}
